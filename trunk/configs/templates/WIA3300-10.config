#
# Base setup
#
CONFIG_VENDOR="WIA"
CONFIG_PRODUCT="WIA3300-10"
CONFIG_FIRMWARE_PROTECT=1       # 固件写保护 (与K2P风格一致)

#
# Platform/SoC Configuration
#
CONFIG_RT_MT7621=y             # 启用MT7621平台 (对齐K2P的注释风格)
# CONFIG_RALINK_MT7620 is not set

#
# Wireless Drivers
#
# CONFIG_MT_WIFI is not set     # 彻底关闭无线功能 (仿照K2P的注释方式)
# CONFIG_FIRST_IF_NONE is not set
# CONFIG_SECOND_IF_NONE is not set

#
# Storage Configuration
#
CONFIG_MTD_STORE_PART_SIZ=0xD00000  # 13MB存储分区 (参考K2P的存储定义方式)
CONFIG_MTD_NFI=y
CONFIG_MTD_NAND=y
CONFIG_MTD_SPI=y

#
# Network Features
#               [K2P风格分组]
#
CONFIG_IPV6=y
CONFIG_L2TP=y
CONFIG_PPTP=y
CONFIG_IPERF3=y                # 保留性能测试工具 (与K2P保持一致)

#
# USB Support
#            [K2P模块化分组]
#
CONFIG_USB=y
CONFIG_USB_EHCI_HCD=y
CONFIG_USB_OHCI_HCD=y
CONFIG_USB_STORAGE=y
CONFIG_USB_PRINTER=y

#
# Filesystem
#
CONFIG_FAT=y                   # 文件系统支持 (与K2P相同配置)
CONFIG_NTFS=y
CONFIG_EXT4=y

#
# Advanced Options
#                [K2P风格高级选项]
#
CONFIG_SYS_LITTLE_ENDIAN=y
CONFIG_SYS_MEM_SIZE=0x1000000  # 内存配置 (16MB ROM对应参数)
CONFIG_SYS_GPIO_BTN_RESET=18   # 复位键GPIO定义 (按实际硬件调整)

